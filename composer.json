{
    "name": "vincere/heatstroke",
    "description": "A minimal Laravel starter-kit. Built with React.js, Inertia.js, Ant Design, Laravel Permission",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vinceredigital/heatstroke/issues",
        "source": "https://github.com/vinceredigital/heatstroke"
    },
    "autoload": {
        "psr-4": {
            "Vincere\\Heatstroke\\": "src/"
        }
    },
    "authors": [
        {
            "name": "I Putu Alfred Crosby",
            "email": "alfred@vinceredigital.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Vincere\\Heatstroke\\HeatstrokeServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.17",
        "illuminate/filesystem": "^10.17",
        "illuminate/support": "^10.17",
        "illuminate/validation": "^10.17"
    }
}
